	<profiles>
		<!--开发环境 -->
		<profile>
			<id>development</id>
			<properties>
				<build.profile.id>development</build.profile.id>
			</properties>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
		</profile>
		<!--临时环境 -->
		<profile>
			<id>temp</id>
			<properties>
				<build.profile.id>temp</build.profile.id>
			</properties>
		</profile>
		<!--测试环境 -->
		<profile>
			<id>test</id>
			<properties>
				<build.profile.id>test</build.profile.id>
			</properties>
		</profile>
		<!--生产环境 -->
		<profile>
			<id>production</id>
